@model ML.Result
@{
    ViewData["Title"] = "BulkCopy";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="container">
    <h1 style="font-family: 'Times New Roman'" class="text-center">Carga Masiva</h1>
    <h5>Seleccione archivo </h5>
    <hr />

    @using (Html.BeginForm("BulkCopy", "CargaMasiva", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <input type="file" class="form-control" name="csv" id="csv" />

        <br />
        <div class="row g-3">
            <div class="col-md-1">
                <a><input type="submit" value="Insertar" class="btn btn-success" /></a>
                <br />
            </div>
        </div>
    }
</div>

